{
    "family": "bsim-check-oauth-client",
    "executionRoleArn": "arn:aws:iam::301868770392:role/bsim-ecs-task-execution-role",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "check-oauth",
            "image": "301868770392.dkr.ecr.ca-central-1.amazonaws.com/bsim/admin:latest",
            "cpu": 0,
            "essential": true,
            "environment": [
                {
                    "name": "DATABASE_URL",
                    "value": "postgresql://bsimadmin:8O9MwSmoA1IUQfOZyw7H4L2lDoeA2M8w@bsim-db.cb80gi4u4k7g.ca-central-1.rds.amazonaws.com:5432/bsim"
                }
            ],
            "command": ["node", "-e", "const{PrismaClient}=require('@prisma/client');const p=new PrismaClient();p.oAuthClient.findUnique({where:{clientId:'ssim-client'}}).then(c=>{console.log('Client:',JSON.stringify(c,null,2));}).catch(e=>console.error(e)).finally(()=>p.$disconnect());"],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/bsim-admin",
                    "awslogs-region": "ca-central-1",
                    "awslogs-stream-prefix": "check-oauth"
                }
            }
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
}
